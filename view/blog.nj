{% extends 'layout.nj' %}

{% block head %}
<link rel="stylesheet" type="text/css" href="/assets/css/blog.css">
{% endblock %}

{% block body %}
<div id="blog">
  <h1>{{title}}</h1>
  <div class="meta">
    {{author}} / {{date}}
  </div>
  <div class="blog-body">
    {{content | safe}}
  </div>
</div>
{% endblock %}
